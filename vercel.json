{
  "redirects": [
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/index",
      "destination": "/",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "/:path*.html"
    }
  ]
}

{
  "redirects": [
    {
      "source": "/:path*.css",
      "destination": "/no-access.html",
      "statusCode": 307
    }
  ]
}
